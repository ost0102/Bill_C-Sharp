<body>

    @{
        if (!string.IsNullOrEmpty(@ViewBag.ContentType))
        {
            <input type="hidden" id="hidden_ContentType" value="@ViewBag.ContentType" />
            <input type="hidden" id="hidden_SearchType" value="@ViewBag.SearchType" />
        }
        else
        {
            <input type="hidden" id="hidden_ContentType" value="B" />
            <input type="hidden" id="hidden_SearchType" value="" />
        }


}

<div id="wrap">
    <div id="header">
        <div class="header">
            @{
                if (Session["USER_ID"] == null)
                {
                    <input type="hidden" id="Session_USR_ID" value="" />
                    <input type="hidden" id="Session_BUSN_NO" value="" />
                    <input type="hidden" id="Session_BUSN_NM" value="" />
                }
                else
                {
                    <input type="hidden" id="Session_USR_ID" value="@Session["USER_ID"]" />
                    <input type="hidden" id="Session_BUSN_NO" value="@Session["BUSN_NO"]" />
                    <input type="hidden" id="Session_BUSN_NM" value="@Session["BUSN_NM"]" />
                }
            }
            <a href="/">
                <img src="~/Images/sub_logo.png" />
            </a>            
            <div class="user_info">
                @{
                    if (Session["BUSN_NM"] != null)
                    {
                        <p class="user_nm">@Session["BUSN_NM"].ToString()</p>
                    }



                }
